<!DOCTYPE html>
<html>

<%- include("./partials/head.ejs") %>


<body id="page-top">
    <div id="wrapper">
        <div class="d-flex flex-column" id="content-wrapper">
            <div id="content">

                <% if (req_auth) { %>
                    <%- include("./partials/nav.loggedin.ejs") %>
                <% } else { %>
                    <%- include("./partials/nav.notlogged.ejs") %>
                <% } %>

                <div class="container-fluid">
                    <div class="card shadow">
                        <div class="card-header py-3">
                            <p class="text-primary m-0 fw-bold"><%=page_name%></p>
                        </div>
                        <div class="card-body">
                            
                            <div class="row">
                                <div class="col-12 col-md-12">
                                    <div class="text-md-end dataTables_filter" id="dataTable_filter">
                                        <label class="form-label"><input type="search" class="form-control form-control-sm" aria-controls="dataTable" placeholder="Search"></label>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="table-responsive table mt-2" id="dataTable" role="grid" aria-describedby="dataTable_info">
                                <%- include("./partials/saved-settings-data.ejs") %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <%- include("./partials/footer_1.ejs") %>
            <div id="snackbar"></div>
        </div>
        <a class="border rounded d-inline scroll-to-top" href="#page-top"><i class="fas fa-angle-up"></i></a>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
    <script src="assets/js/script.min.js"></script>
    <script src="https://cdn.socket.io/3.1.3/socket.io.min.js" 
        integrity="sha384-cPwlPLvBTa3sKAgddT6krw0cJat7egBga3DJepJyrLl4Q9/5WLra3rrnMcyTyOnh" 
        crossorigin="anonymous">
    </script>
    <script src="/assets/js/socketClientHandlerSavedSettings.js"></script>
</body>

</html>